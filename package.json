{
  "name": "routinely",
  "version": "0.0.1",
  "scripts": {
    "dev": "nodemon dist/server/index.ts",
    "build": "npm run build:client && npm run build:server",
    "watch": "concurrently \"npm run watch:client\" \"npm run watch:server\"",
    "watch:client": "parcel watch -d dist/client src/client/index.html --public-url .",
    "watch:server": "nodemon -w src/server/**/* -x 'npm run build:server && node dist/server'",
    "build:client": "parcel build -d dist/client src/client/index.html --public-url .",
    "build:server": "tsc -p tsconfig.server.json",
    "start": "npm run build && NODE_ENV=production node dist/server/index.js",
    "test": "NODE_ENV=testing jest --watch"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.1.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.0-14",
    "@fortawesome/free-solid-svg-icons": "^5.1.0-11",
    "autoprefixer": "^8.2.0",
    "babel-jest": "^22.4.3",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "dotenv": "^5.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "express": "^4.16.3",
    "express-async-handler": "^1.1.3",
    "express-promise-router": "^3.0.1",
    "express-session": "^1.15.6",
    "express-static": "^1.2.5",
    "http-errors": "^1.6.3",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^22.4.3",
    "jest-enzyme": "^6.0.0",
    "jest-fetch-mock": "^1.5.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "mongoose": "^5.0.18",
    "passport": "^0.4.0",
    "passport-auth0": "^0.6.1",
    "postcss": "^6.0.21",
    "postcss-easy-import": "^3.0.0",
    "postcss-url": "^7.3.2",
    "prettier": "^1.11.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "reactstrap": "^6.3.0",
    "supertest": "^3.0.0",
    "ts-jest": "^22.4.2",
    "ts-node": "^4.1.0",
    "urijs": "^1.19.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/connect-mongo": "0.0.37",
    "@types/cookie-parser": "^1.4.1",
    "@types/dotenv": "^4.0.3",
    "@types/enzyme": "^3.1.9",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/express": "^4.16.0",
    "@types/http-errors": "^1.6.1",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/jest": "^22.2.2",
    "@types/lodash": "^4.14.112",
    "@types/node": "^10.0.6",
    "@types/passport": "^0.4.5",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "@types/reactstrap": "^5.0.27",
    "@types/supertest": "^2.0.4",
    "@types/urijs": "^1.15.38",
    "bootstrap": "^4.1.2",
    "concurrently": "^3.6.1",
    "node-sass": "^4.9.2",
    "nodemon": "^1.18.3",
    "parcel-bundler": "^1.9.7",
    "typescript": "^2.9.2"
  }
}
